# platform :ios, '9.0'

target 'Fresh Sauce' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Fresh Sauce
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'
  pod 'Firebase/Auth'

  target 'Fresh SauceTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Fresh SauceUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
